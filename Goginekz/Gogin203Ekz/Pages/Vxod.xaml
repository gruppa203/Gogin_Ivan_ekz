<Page x:Class="Gogin203Ekz.Pages.Vxod"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gogin203Ekz.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Vxod">

    

        <Grid Background="White" MinWidth="400" MinHeight="200">

        <StackPanel HorizontalAlignment="Center" Height="200"  Width="790">
            <Label Content="Логин" HorizontalAlignment="Center"  VerticalAlignment="Top"/>
            <TextBox x:Name="Log" Height="23" TextWrapping="Wrap" Text="Введите логин" HorizontalAlignment="Center" MinWidth="100"/>
            <Label Content="Пароль" HorizontalAlignment="Center"  VerticalAlignment="Top"/>
            <TextBox x:Name="Par" Height="23" TextWrapping="Wrap" Text="Введите пароль" HorizontalAlignment="Center" MinWidth="100"/>
            <Button x:Name="vxod_button" Content="Вход" HorizontalAlignment="Center" Click="Button_Click"/>

        </StackPanel>
    </Grid>
</Page>
